{
  "name": "onefinity-firmware",
  "version": "1.0.0",
  "description": "Onefinity CNC Controller",
  "main": "../../src/js/main.js",
  "dependencies": {
    "vue": "^2.6.14",
    "sockjs-client": "^1.5.0",
    "jquery": "^1.11.3",
    "three": "^0.125.0",
    "clusterize.js": "^0.18.1"
  },
  "devDependencies": {
    "pug": "^3.0.2",
    "pug-cli": "^1.0.0-alpha6",
    "pug-filters": "^4.0.0",
    "stylus": "^0.54.8",
    "jstransformer-stylus": "^1.5.0",
    "browserify": "^17.0.0",
    "watchify": "^4.0.0"
  },
  "scripts": {
    "build": "mkdir -p ../../dist && mkdir -p ../../dist/js && pug ../../src/pug/index.pug -o ../../dist && browserify ../../src/js/main.js -o ../../dist/js/app.js",
    "watch": "pug -w ../../src/pug/index.pug -o ../../dist & watchify ../../src/js/main.js -o ../../dist/js/app.js"
  }
}
